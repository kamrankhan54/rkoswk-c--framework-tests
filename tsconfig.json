{
    "compilerOptions": {
      "resolveJsonModule": true,
      "target": "ES2020",
      "module": "commonjs",
      "moduleResolution": "node",
      "types": ["node", "@playwright/test"],
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": [
    "**/*.ts",
    "**/*.js",
    "features/**/*.feature",
    "steps/**/*.ts",
    "pages/**/*.ts"
  ]
  }
  